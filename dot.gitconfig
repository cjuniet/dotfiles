[alias]
  br = branch
  ci = commit
  co = checkout
  fap = fetch --all --prune
  ft = log --all --graph --decorate --oneline
  last = log -1 --stat
  review = push gerrit HEAD:refs/publish/master
  st = status -sb
  sync = fetch origin master:master
  tree = log -16 --graph --decorate --oneline
[branch]
  autosetuprebase = always
[color]
  ui = true
[push]
  default = simple
